{
    "name":"travisstroud/yiiSkeletonApp",
    "description":"A simple Yii application with user management.",
    "authors":[
        {
            "name":"Travis Stroud",
            "email":"travis@travisstroud.com"
        }
    ],
    "repositories":[
        {"type":"composer", "url":"http://packages.phundament.com"},
        {"type": "package",
            "package": {
                "name": "dknight/jQuery-Notify-bar",
                "version": "1.0.0",
                "source": {
                    "url": "git://github.com/dknight/jQuery-Notify-bar.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require":{
        "php":">=5.3.2",
        "crisu83/yii-bootstrap":"2.0.3",
        "crisu83/yii-less":"dev-master",
        "dknight/jQuery-Notify-bar":"1.0.0",
        "yiisoft/yii":"1.1.14"
    },
    "require-dev":{
        "phpunit/phpunit":"3.7.*"
    },
    "autoload":{
        "psr-0":{
            "App":"app"
        }
    }
}